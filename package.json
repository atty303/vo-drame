{
  "name": "panel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "base-x": "^3.0.4",
    "comlinkjs": "^3.0.3",
    "csinterface-ts": "^1.0.3",
    "extendscript-es5-shim-ts": "^0.0.1",
    "immutable": "^3.8.2",
    "noice-json-rpc": "^1.2.0",
    "tsmonad": "^0.8.0",
    "ve": "^0.1.3",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vuetify": "^1.1.4"
  },
  "devDependencies": {
    "@types/base-x": "^1.0.29",
    "@types/node": "7",
    "@vue/component-compiler-utils": "^2.1.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.6",
    "stylus": "^0.54.5",
    "tslint": "^5.10.0",
    "types-for-adobe": "github:pravdomil/types-for-adobe",
    "typescript": "^2.9.2",
    "vue-template-compiler": "^2.5.16"
  },
  "scripts": {
    "develop": "run-p develop:*",
    "develop:browser": "parcel watch --out-file browser --public-url . --hmr-hostname=localhost src/browser/main.html",
    "develop:host": "parcel watch --out-file host --no-hmr src/host/main.ts",
    "_watch:panel": "parcel watch --out-dir panel/dist --out-file bundle.js --hmr-hostname localhost panel/src/main.ts",
    "_watch:host": "parcel watch --out-dir host/dist --no-hmr host/src/main.ts",
    "build": "run-s build:*",
    "build:browser": "parcel build --out-file browser --public-url . src/browser/main.html"
  }
}
